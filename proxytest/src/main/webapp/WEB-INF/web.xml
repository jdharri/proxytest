<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>proxyservlet</servlet-name>
        <servlet-class>org.mitre.dsmiley.httpproxy.ProxyServlet</servlet-class>
        <init-param>
            <param-name>targetUri</param-name>
            <param-value>https://localhost.localdomain:8181/maven</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>gf</servlet-name>
        <servlet-class>org.mitre.dsmiley.httpproxy.ProxyServlet</servlet-class>
        <init-param>
            <param-name>targetUri</param-name>
            <param-value>https://localhost.localdomain:4848/</param-value>
        </init-param>
    </servlet>
     <servlet>
        <servlet-name>gmail</servlet-name>
        <servlet-class>org.mitre.dsmiley.httpproxy.ProxyServlet</servlet-class>
        <init-param>
            <param-name>targetUri</param-name>
            <param-value>https://accounts.google.com/</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>proxyservlet</servlet-name>
        <url-pattern>/mav/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gf</servlet-name>
        <url-pattern>/gf/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gmail</servlet-name>
        <url-pattern>/gmail/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
